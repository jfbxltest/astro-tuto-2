---
import Social from "./Social.astro"
const socials = [
    { platform: "twitter", username: "astrodotbuild" },
    { platform: "github", username: "withastro" },
    { platform: "youtube", username: "astrodotbuild" },
]
---

<style>
    footer {
        display: flex;
        gap: 1rem;
        margin-top: 2rem;
    }
</style>
<footer>
    <p>Apprenez plus sur mes projets</p>
    {socials.map((s) => <Social platform={s.platform} username={s.username} />)}
</footer>
